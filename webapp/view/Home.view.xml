<mvc:View controllerName="licenta.controller.Home" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m" xmlns:c="sap.ui.core" xmlns:unified="sap.ui.unified" xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">

    <SplitApp id="homeSplitAppID">
        <detailPages>
            <Page id="articlesSplitID" title="Articles">
                <Label id="articlesLabelID" text="articles"/>
            </Page>
            <Page id="myTherapistSplitID" title=" My Therapist"></Page>
            <Page id="therapistsSplitID" title=" My Therapist"></Page>
            <Page id="profileSplitID" title="Profile"></Page>
            <Page id="calendarSplitID" title="Calendar">
            </Page>
            <Page id="accountSplitID" title="Account"></Page>
        </detailPages>
        <masterPages>
            <Page id="masterHome" backgroundDesign="List">
                <List id="listHomeID" itemPress=".onListItemPress">
                    <items>
                        <StandardListItem id="articlesListID" title="Articles" type="Active" custom:to="articlesSplitID"/>
                        <StandardListItem id="therapistsListID" title=" All Therapists" type="Active" custom:to="therapistsSplitID"/>
                        <StandardListItem id="myTherapistListID" title=" My Therapist" type="Navigation" press=".onPressGoToMyTherapist"/>
                        <StandardListItem id="accountListID" title="Account" type="Active" custom:to="accountSplitID"/>
                    </items>
                </List>
            </Page>
            <Page id="masterMyTherapist" title="Home" backgroundDesign="List" showNavButton="true" navButtonPress=".onPressMasterBack">
                <List id="listMyTherapistID" itemPress=".onListItemPress">
                    <items>
                        <StandardListItem id="profileListID" title="Profile" type="Active" custom:to="profileSplitID"/>
                        <StandardListItem id="calendarListID" title="Calendar" type="Active" custom:to="calendarSplitID"/>
                    </items>
                </List>
            </Page>
        </masterPages>
    </SplitApp>
</mvc:View>
