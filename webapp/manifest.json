{
    "_version": "1.32.0",
    "sap.app": {
        "id": "licenta",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "Licenta",
        "description": "Description",
        "dataSources": {
            "mainService": {
                "uri": "/sap/opu/odata/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "4.0"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "rootView": {
            "viewName": "licenta.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        },
        "dependencies": {
            "minUI5Version": "1.98.0",
            "libs": {
                "sap.ui.core": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "licenta.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true,
                    "groupId": "$direct"
                }
            },
            "category": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "utils/Categories.json"
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "licenta.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "Welcome",
                    "pattern": "",
                    "target": "Welcome"
                },
                {
                    "name": "Login",
                    "pattern": "Login",
                    "target": ["Login"]
                },
                {
                    "name": "Register",
                    "pattern": "Register",
                    "target": ["Register"]
                },
                {
                    "name": "Home",
                    "pattern": "Home/{token}",
                    "target": ["Home"]
                }, {
                    "name": "Account",
                    "pattern": "Account",
                    "target": ["Account"]
                }, {
                    "name": "HomeTherapist",
                    "pattern": "HomeTherapist/{token}",
                    "target": ["HomeTherapist"]
                }
            ],
            "targets": {
                "Welcome": {
                    "viewType": "XML",
                    "viewName": "Welcome"
                },
                "Login": {
                    "viewType": "XML",
                    "viewName": "Login"
                },
                "Register": {
                    "viewType": "XML",
                    "viewName": "Register"
                },
                "Home": {
                    "viewType": "XML",
                    "viewName": "Home"
                },
                "Account": {
                    "viewType": "XML",
                    "viewName": "Account"
                },
                "HomeTherapist": {
                    "viewType": "XML",
                    "viewName": "HomeTherapist"
                }
            }
        }
    }
}
